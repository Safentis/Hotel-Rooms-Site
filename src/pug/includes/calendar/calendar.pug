div(class="calendar calendar_off", id="calendar")
    div(class="calendar__header", id="calendar-head")
    span(class="calendar__arrow arrow-left", data-arrow="left") 
    span(class="calendar__arrow arrow-right", data-arrow="right")
    table(class="calendar__table")
        tbody(class="calendar__body", id="calendar-body")
    div(class="calendar__buttons buttons")
        button(class="button-pick button-clear", type="button", data-button="cleare-pick") 
            | Очистить
        button(class="button-pick button-apply", type="button" data-button="apply-pick") 
            | Применить